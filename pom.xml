<project xmlns="http://maven.apache.org/POM/4.0.0" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://maven.apache.org/POM/4.0.0 http://maven.apache.org/xsd/maven-4.0.0.xsd">
	<modelVersion>4.0.0</modelVersion>
	<parent>
		<groupId>org.springframework.boot</groupId>
		<artifactId>spring-boot-starter-parent</artifactId>
		<version>1.5.3.RELEASE</version>
		<relativePath />
	</parent>
	<groupId>com.ymu.spcselling</groupId>
	<artifactId>ymu-special-selling</artifactId>
	<version>1.0.0</version>
	<packaging>pom</packaging>
	<name>ymu-special-selling</name>
	<description>
		spring-cloud微服务架构
	</description>

	<properties>
		<project.build.sourceEncoding>UTF-8</project.build.sourceEncoding>
		<java.version>1.8</java.version>
		<dubbo.version>2.5.3</dubbo.version>
		<zookeeper.version>3.4.6</zookeeper.version>
		<swagger.version>2.6.1</swagger.version>
		<druid.version>1.0.25</druid.version>
		<mysql.connector.version>5.1.21</mysql.connector.version>
		<joda.time.version>2.9.7</joda.time.version>
		<guava.version>20.0</guava.version>
		<kaptcha.version>0.0.9</kaptcha.version>
		<mybatis.spring.boot.version>1.1.1</mybatis.spring.boot.version>

		<apache.common.beanutils>1.9.3</apache.common.beanutils>
	</properties>

	<!-- 阿里云maven仓库 -->
	<repositories>
		<repository>
			<id>public</id>
			<name>aliyun nexus</name>
			<url>http://maven.aliyun.com/nexus/content/groups/public/</url>
			<releases>
				<enabled>true</enabled>
			</releases>
		</repository>
	</repositories>

	<modules>
		<module>mservice-apis</module>
		<module>mservice-services</module>
		<module>mservice-webs</module>
		<module>mservice-infrastructure</module>
		<module>spcselling-services</module>
		<module>spcselling-webs</module>
		<module>spcselling-infrastructure</module>
		<!-- <module>spcselling-config</module> -->

		<module>spcselling-config</module>
		<module>spcselling-gateway</module>
	</modules>

	<dependencies>
		<!-- spring boot -->
		<dependency>
			<groupId>org.springframework.boot</groupId>
			<artifactId>spring-boot-starter</artifactId>
		</dependency>

		<dependency>
			<groupId>org.springframework.boot</groupId>
			<artifactId>spring-boot-starter-test</artifactId>
			<scope>test</scope>
		</dependency>

		<!-- log4j日志 -->
		<dependency>
			<groupId>org.springframework.boot</groupId>
			<artifactId>spring-boot-starter</artifactId>
			<exclusions>
				<exclusion>
					<groupId>org.springframework.boot</groupId>
					<artifactId>spring-boot-starter-logging</artifactId>
				</exclusion>
			</exclusions>
		</dependency>
		<dependency>
			<groupId>org.springframework.boot</groupId>
			<artifactId>spring-boot-starter-log4j2</artifactId>
		</dependency>

	</dependencies>

	<dependencyManagement>
		<dependencies>
			<!-- 项目模块应用 -->
			<dependency>
				<groupId>${project.groupId}</groupId>
				<artifactId>ymu-api</artifactId>
				<version>${project.version}</version>
				<exclusions>
					<exclusion>
						<groupId>org.springframework.boot</groupId>
						<artifactId>spring-boot-starter-data-jpa</artifactId>
					</exclusion>
				</exclusions>
			</dependency>
			<dependency>
				<groupId>${project.groupId}</groupId>
				<artifactId>ymu-infrastructure</artifactId>
				<version>${project.version}</version>
			</dependency>
			<dependency>
				<groupId>${project.groupId}</groupId>
				<artifactId>ymu-webui-common</artifactId>
				<version>${project.version}</version>
			</dependency>

			<!-- Apache common start -->
			<dependency>
				<groupId>commons-beanutils</groupId>
				<artifactId>commons-beanutils</artifactId>
				<version>${apache.common.beanutils}</version>
			</dependency>
			<!-- Apache common end -->

			<dependency>
				<groupId>mysql</groupId>
				<artifactId>mysql-connector-java</artifactId>
				<version>${mysql.connector.version}</version><!--$NO-MVN-MAN-VER$ -->
			</dependency>

			<dependency>
				<groupId>com.alibaba</groupId>
				<artifactId>druid</artifactId>
				<version>${druid.version}</version>
			</dependency>

			<!--日期，时间处理 -->
			<!--http://www.joda.org/ -->
			<dependency>
				<groupId>joda-time</groupId>
				<artifactId>joda-time</artifactId>
				<version>${joda.time.version}</version>
			</dependency>

			<!--https://github.com/google/guava -->
			<dependency>
				<groupId>com.google.guava</groupId>
				<artifactId>guava</artifactId>
				<version>${guava.version}</version>
			</dependency>

			<!--图形验证码 -->
			<dependency>
				<groupId>com.github.axet</groupId>
				<artifactId>kaptcha</artifactId>
				<version>${kaptcha.version}</version>
			</dependency>

		</dependencies>

	</dependencyManagement>

	<build />
	
</project>
